{
  "name": "cars-test-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "client": "nodemon client/app.ts --watch client",
    "server": "nodemon src/server.ts --watch src",
    "build": "webpack --config webpack.server.js && npx pkg --o build/setup -t node16-linuxstatic -C GZip ./build/server.js && webpack --config webpack.client.js && npx pkg --o build/public/app/setup -t node16-win -C GZip ./build/app.js && npx pkg --o build/public/app/setup -t node16-linux -C GZip ./build/app.js",
    "pretty": "npx prettier src client --write"
  },
  "keywords": [
    "auto",
    "car",
    "api"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.1",
    "@types/node-fetch": "^2.6.5",
    "filemanager-webpack-plugin": "^8.0.0",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.1",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "moment": "^2.29.4",
    "mongodb": "^4.11.0",
    "mongoose": "^6.8.1",
    "node-fetch": "^2.7.0"
  },
  "author": "HukakHePak",
